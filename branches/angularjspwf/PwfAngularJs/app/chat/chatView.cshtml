<h2>Chat View</h2>

<div class="well chat-container">
    <div class="row">
        <div class="col-md-9">test overlap</div>
        <div class="col-md-3">
            <ul>
                <li ng-repeat="player in vm.players">
                    {{player.Name}}
                </li>
            </ul>
        </div>
    </div>

    <div class="row">
        <form ng-submit="vm.join(userName)">
            <input type="text" ng-model="userName" />
            <input type="submit" />
        </form>
    </div>
</div>
